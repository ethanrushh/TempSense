{
  "server_endpoint": "http://localhost:5058/hub",
  "page_id": "Dev PC",
  
  "pages": [
    {
      "page_name": "Extern Senors",
      
      "rows": "2*, 2*, 2*, *",
      "cols": "*, *",
      
      "widget_layout": "MinisWithFooter",
      
      "widgets": [
        {
          "type": "Sensor",
          "widget_display_type": "Round",

          "device_name": "coretemp-isa-0000",
          "sensor_name": "Core 0",
          "field_name": "temp2_input",

          "display_name": "Page 2 Sensor",
          "unit": "°C"
        },
        {
          "type": "Sensor",
          "widget_display_type": "Round",

          "device_name": "coretemp-isa-0000",
          "sensor_name": "Package id 0",
          "field_name": "temp1_input",

          "display_name": "CPU Package (°C)",
          "unit": "°C"
        },
        {
          "type": "Disk",
          "widget_display_type": "Readout",

          "device_name": "/",

          "display_name": "Free Space (GiB)"
        },
        {
          "type": "Memory",
          "widget_display_type": "Fluid",

          "memory_sensor_target": "UsedPercentage",

          "display_name": "Memory Usage"
        },
        {
          "type": "Network",
          "widget_display_type": "Readout",
          "header": "↑",

          "device_name": "enp4s0",
          "direction": "Up",

          "display_name": "enp4s0 Upload"
        },
        {
          "type": "Network",
          "widget_display_type": "Readout",
          "header": "↓",

          "device_name": "enp4s0",
          "direction": "Down",

          "display_name": "enp4s0 Download"
        },

        {
          "type": "Memory",
          "widget_display_type": "Readout",

          "memory_sensor_target": "Total",

          "display_name": "Total Memory"
        },
        {
          "type": "Memory",
          "widget_display_type": "Readout",

          "memory_sensor_target": "Free",

          "display_name": "Available RAM"
        }
      ]
    },


    {
      "page_name": "Core Senors",

      "rows": "2*, 2*, 2*, *",
      "cols": "*, *",

      "widget_layout": "MinisWithFooter",

      "widgets": [
        {
          "type": "Sensor",
          "widget_display_type": "Round",

          "device_name": "coretemp-isa-0000",
          "sensor_name": "Core 0",
          "field_name": "temp2_input",

          "display_name": "CPU Core 0 Temp",
          "unit": "°C"
        },
        {
          "type": "Sensor",
          "widget_display_type": "Round",

          "device_name": "coretemp-isa-0000",
          "sensor_name": "Package id 0",
          "field_name": "temp1_input",

          "display_name": "CPU Package (°C)",
          "unit": "°C"
        },
        {
          "type": "Disk",
          "widget_display_type": "Readout",

          "device_name": "/",

          "display_name": "Free Space (GiB)"
        },
        {
          "type": "Memory",
          "widget_display_type": "Fluid",

          "memory_sensor_target": "UsedPercentage",

          "display_name": "Memory Usage"
        },
        {
          "type": "Network",
          "widget_display_type": "Readout",
          "header": "↑",

          "device_name": "enp4s0",
          "direction": "Up",

          "display_name": "enp4s0 Upload"
        },
        {
          "type": "Network",
          "widget_display_type": "Readout",
          "header": "↓",

          "device_name": "enp4s0",
          "direction": "Down",

          "display_name": "enp4s0 Download"
        },

        {
          "type": "Memory",
          "widget_display_type": "Readout",

          "memory_sensor_target": "Total",

          "display_name": "Total Memory"
        },
        {
          "type": "Memory",
          "widget_display_type": "Readout",

          "memory_sensor_target": "Free",

          "display_name": "Available RAM"
        }
      ]
    }
  ]
}
